<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script src="https://cdn.tailwindcss.com"></script>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;600;700&display=swap" rel="stylesheet">
    <title>Show Page</title>
    <style>
      * {
        font-family: 'Inter', sans-serif;
        margin: 0;
        padding: 0;
        outline: 0;
        background-color: #EEF1F5;
      }
    </style>
  </head>
  <body>
    <div class=" container flex flex-row border-solid w-full mx-auto mt-[25px] bg-white h-[80px] rounded-[12px]  ">
      <a class="bg-inherit self-center ms-[24px] me-[16px]" href="/admin">
        <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
          <path d="M12 22C17.5228 22 22 17.5228 22 12C22 6.47715 17.5228 2 12 2C6.47715 2 2 6.47715 2 12C2 17.5228 6.47715 22 12 22Z" stroke="#1960EA" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
          <path d="M12 8L8 12L12 16" stroke="#1960EA" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
          <path d="M16 12H8" stroke="#1960EA" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
          </svg>
      </a>
      <div class="bg-inherit font-bold self-center text-[20px] leading-[28px]">User Details</div>
    </div>
    <div class="container flex flex-row w-full mx-auto mt-[16px]">
      <div class="container w-1/3 bg-inherit p-[24px] me-[20px] border rounded-[12px] bg-white">
        <div class="bg-inherit font-bold self-center text-[20px] leading-[28px] ">User Informations</div>
        <hr class="h-px my-[24px] bg-[#E5E5E5]">
          <table class=" w-full bg-inherit border-separate border-spacing-x-[20px]">
            <tbody class="bg-inherit">
              <tr class="bg-inherit h-[40px]">
                <td class="bg-inherit">User Id</td>
                <td class="bg-inherit font-bold"><%= data.id %></td>
              </tr>
              <tr class="bg-inherit h-[40px]">
                <td class="bg-inherit">Username</td>
                <td class="bg-inherit font-bold"><%= data.username %></td>
              </tr>
              <tr class="bg-inherit h-[40px]">
                <td class="bg-inherit">Password</td>
                <td class="bg-inherit font-bold"><%= data.password %></td>
              </tr>
              <tr class="bg-inherit h-[40px]">
                <td class="bg-inherit">Full Name</td>
                <td class="bg-inherit font-bold"><%= data.UserBiodatum.fullname %></td>
              </tr>
              <tr class="bg-inherit h-[40px]">
                <td class="bg-inherit">City</td>
                <td class="bg-inherit font-bold"><%= data.UserBiodatum.city %></td>
              </tr>
              <tr class="bg-inherit h-[40px]">
                <td class="bg-inherit">Birthday</td>
                <td class="bg-inherit font-bold"><%= data.UserBiodatum.birthday %></td>
              </tr>
              <tr class="bg-inherit h-[40px]">
                <td class="bg-inherit">Gender</td>
                <td class="bg-inherit font-bold"><%= data.UserBiodatum.gender %></td>
              </tr>
              <tr class="bg-inherit h-[40px]">
                <td class="bg-inherit">Created At</td>
                <td class="bg-inherit font-bold"><%= data.createdAt %></td>
              </tr>
              <tr class="bg-inherit h-[40px]">
                <td class="bg-inherit">Updated At</td>
                <td class="bg-inherit font-bold"><%= data.updatedAt %></td>
              </tr>
            </tbody>
          </table>
      </div>
      <div class="container w-2/3 bg-inherit p-[24px] border rounded-[12px] bg-white">
        <div class="bg-inherit font-bold self-center text-[20px] leading-[28px] ">Game History</div>
        <hr class="h-px my-[24px] bg-[#E5E5E5] border-0">
        <table class="container mx-auto table-auto mt-[16px] border-separate border-spacing-x-[40px] bg-white">
          <thead class="bg-white">
            <tr class=" bg-inherit text-[#5A5A5A] text-left h-[52px]">
              <th scope="col" class="bg-inherit font-normal">User ID</th>
              <th scope="col" class="bg-inherit font-normal">Score</th>
              <th scope="col" class="bg-inherit font-normal">Time</th>
            </tr>
          </thead>
          <tbody class="bg-white font-semibold ">
            <% for(var i=0; i < data.UserHistories.length; i++) { %>
            <tr class="bg-inherit h-[52px]">
              <td class="bg-inherit"><%= data.UserHistories[i].id %></td>
              <td class="bg-inherit"><%= data.UserHistories[i].score %></td>
              <td class="bg-inherit"><%= data.UserHistories[i].createdAt %></td>
            </tr>
            <% } %>
        </table>
      </div>
    </div>
  </body>
</html>
